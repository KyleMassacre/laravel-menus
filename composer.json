{
  "name": "kylemassacre/laravel-menus",
  "description": "Laravel Menu management",
  "keywords": [
    "laravel",
    "menus",
    "navigation",
    "nav",
    "bootstrap"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Nicolas Widart",
      "email": "n.widart@gmail.com"
    },
    {
      "name": "Pingpong Labs",
      "email": "pingpong.labs@gmail.com"
    },
    {
      "name": "Kyle Ellis",
      "email": "ky.ellis83@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/support": "^11.0",
    "illuminate/config": "^11.0",
    "illuminate/view": "^11.0",
    "spatie/laravel-html": "^3.9"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.1",
    "mockery/mockery": "^1.6",
    "orchestra/testbench": "^9.0",
    "friendsofphp/php-cs-fixer": "^3.54"
  },
  "autoload": {
    "psr-4": {
      "KyleMassacre\\Menus\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "KyleMassacre\\Menus\\Tests\\": "Tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "9.0-dev"
    },
    "laravel": {
      "providers": [
        "KyleMassacre\\Menus\\MenusServiceProvider"
      ],
      "aliases": {
        "Menu": "KyleMassacre\\Menus\\Facades\\Menu"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "php-cs-fixer": "php-cs-fixer fix --config=./.php-cs-fixer.dist.php",
    "run-tests": "./vendor/bin/phpunit "
  }
}
