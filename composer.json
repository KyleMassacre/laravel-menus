{
  "name": "kylemassacre/laravel-menus",
  "description": "Laravel Menu management",
  "keywords": [
    "laravel",
    "menus",
    "navigation",
    "nav",
    "bootstrap"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Nicolas Widart",
      "email": "n.widart@gmail.com"
    },
    {
      "name": "Pingpong Labs",
      "email": "pingpong.labs@gmail.com"
    },
    {
      "name": "Kyle Ellis",
      "email": "ky.ellis83@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/support": "^10.0",
    "illuminate/config": "^10.0",
    "illuminate/view": "^10.0",
    "laravelcollective/html": "^6.4"
  },
  "require-dev": {
    "phpunit/phpunit": ">=9.6",
    "mockery/mockery": "^1.6",
    "orchestra/testbench": "^8",
    "friendsofphp/php-cs-fixer": "^3.52"
  },
  "autoload": {
    "psr-4": {
      "KyleMassacre\\Menus\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "KyleMassacre\\Menus\\Tests\\": "Tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "6.0-dev"
    },
    "laravel": {
      "providers": [
        "KyleMassacre\\Menus\\MenusServiceProvider"
      ],
      "aliases": {
        "Menu": "KyleMassacre\\Menus\\Facades\\Menu"
      }
    }
  },
  "minimum-stability": "stable",
  "scripts": {
    "php-cs-fixer": "php-cs-fixer --config=./.php-cs-fixer.dist.php"
  }
}
