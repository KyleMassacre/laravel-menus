{
  "name": "kylemassacre/laravel-menus",
  "description": "Laravel Menu management",
  "keywords": [
    "laravel",
    "menus",
    "navigation",
    "nav",
    "bootstrap"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Nicolas Widart",
      "email": "n.widart@gmail.com"
    },
    {
      "name": "Pingpong Labs",
      "email": "pingpong.labs@gmail.com"
    },
    {
      "name": "Kyle Ellis",
      "email": "ky.ellis83@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/support": "^9.21|^10.0|^12.0",
    "illuminate/config": "^9.21|^10.0|^12.0",
    "illuminate/view": "^9.21|^10.0|^12.0",
    "spatie/laravel-html": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.1",
    "mockery/mockery": "^1.6.0",
    "orchestra/testbench": "^8.10.0|^9.0",
    "friendsofphp/php-cs-fixer": "^3.40.0"
  },
  "autoload": {
    "psr-4": {
      "KyleMassacre\\Menus\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "KyleMassacre\\Menus\\Tests\\": "Tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "6.0-dev"
    },
    "laravel": {
      "providers": [
        "KyleMassacre\\Menus\\MenusServiceProvider"
      ],
      "aliases": {
        "Menu": "KyleMassacre\\Menus\\Facades\\Menu"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "php-cs-fixer": "PHP_CS_FIXER_IGNORE_ENV=1 php-cs-fixer fix --config=./.php-cs-fixer.dist.php"
  },
  "suggest": {
    "spatie/laravel-html": "A modern alternative to laravelcollective/html which is no longer supported"
  }
}
